{
	"publisher": "StackBuild",
	"name": "bazel-stack-vscode-go",
	"displayName": "bazel-stack-vscode-go",
	"description": "Rules-Go Support For Bazel Stack VSCode Extension",
	"version": "1.1.1",
	"icon": "stackb-full.png",
	"engines": {
		"vscode": "^1.45.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/stackb/bazel-stack-vscode-go.git"
	},
	"keywords": [
		"bazel"
	],
	"author": "stack.build authors",
	"bugs": {
		"url": "https://github.com/stackb/bazel-stack-vscode-go/issues"
	},
	"homepage": "https://github.com/stackb/bazel-stack-vscode-go#readme",
	"categories": [
		"Programming Languages",
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"extensionDependencies": [
		"stackbuild.bazel-stack-vscode"
	],
	"main": "./out/extension.js",
	"contributes": {
		"problemMatchers": [
			{
				"name": "GoCompilePkg",
				"fileLocation": [
					"relative",
					"${workspaceRoot}"
				],
				"pattern": {
					"regexp": "^(?:.*/execroot/[_a-zA-Z0-9]+/)(.*):(\\d+):(\\d+):\\s+(.*)$",
					"file": 1,
					"line": 2,
					"column": 3,
					"message": 4
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode-install": "vsce package --out ext.vsix && code --install-extension ext.vsix"
	},
	"devDependencies": {
		"@types/vscode": "^1.45.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"@typescript-eslint/parser": "^2.30.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-common": "1.49.0",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"bazel-stack-vscode-api": "^1.2.2"
	}
}
